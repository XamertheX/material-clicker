{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/.webpack": true,
    "**/.npmrc": true,
    "**/.babelrc": true,
    "**/.vscode": true,
    "**/.eslintcache": true,
    "**/.eslintrc": true,
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.webpack": true,
    "**/out/**": true,
    "**/.eslintcache": true,
  },
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.rulers": [90]
}