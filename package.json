{
  "name": "material-clicker",
  "productName": "material-clicker",
  "version": "0.0.1",
  "description": "Clicker Game using Material Design",
  "main": "./.webpack/main",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "electron-forge start",
    "env-production": "run-script-os",
    "env-production:win32": "SET NODE_ENV=production && npm run package:start",
    "env-production:linux:darwin": "export NODE_ENV=production && npm run package:start",
    "package": "npm run env-production && electron-forge package",
    "make": "npm run env-production && electron-forge make",
    "publish": "npm run env-production && electron-forge publish",
    "lint": "eslint . --ext js,jsx",
    "cloc": "cloc src conf",
    "docs": "docsify serve docs"
  },
  "keywords": [],
  "author": "WeAreDevs",
  "contributors": [
    {
      "name": "Dave",
      "url": "https://davecode.me"
    },
    {
      "name": "Hunter",
      "url": "https://hparcells.me"
    }
  ],
  "license": "MIT",
  "config": {
    "forge": "./conf/forge.config.js"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@reach/router": "^1.2.1",
    "classnames": "^2.2.6",
    "comma-number": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "eventemitter3": "^3.1.0",
    "path-browserify": "^1.0.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-hot-loader": "^4.6.5"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime-corejs2": "^7.3.1",
    "@electron-forge/cli": "^6.0.0-beta.33",
    "@electron-forge/maker-deb": "^6.0.0-beta.33",
    "@electron-forge/maker-rpm": "^6.0.0-beta.33",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.33",
    "@electron-forge/maker-zip": "^6.0.0-beta.33",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.22",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "docsify-cli": "^4.3.0",
    "electron": "4.0.4",
    "electron-react-devtools": "^0.5.3",
    "eslint": "^5.13.0",
    "eslint-config-dave": "^1.0.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.3"
  }
}
